{
    "family": "magic-8ball-task",
    "networkMode": "bridge",
    "requiresCompatibilities": ["EC2"],
    "cpu": "512",
    "memory": "512",
    "executionRoleArn": "ROLE ARN",
    "runtimePlatform": {
        "operatingSystemFamily": "LINUX",
        "cpuArchitecture": "X86_64"
    },
    "containerDefinitions": [
        {
            "name": "magic-8ball-container",
            "image": "ECR IMAGE",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}